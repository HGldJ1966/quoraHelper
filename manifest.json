{
    "manifest_version": 2,
    "name": "Quora Helper",
    "version": "0.1",

    "description": "This extension helps you using j/k to move around on Quora.com, and using Enter to help you open comments.",

    "author": "revir.qing@gmail.com",
    "background": {
        "script": "js/background/main.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.quora.com/*"
            ],
            "js": [
                "bower_components/jquery/dist/jquery.min.js",
                "js/content/main.js"
            ]
        }
    ]
}

